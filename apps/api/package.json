{
  "name": "@apps/api",
  "version": "0.0.0",
  "private": true,
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "dev": "pnpm exec nest start --watch --path tsconfig.json --builder tsc",
    "lint": "eslint src --ext .ts",
    "test": "pnpm exec ts-node --project tsconfig.test.json test/streaming.test.ts",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@packages/agents": "workspace:*",
    "@packages/config": "workspace:*",
    "@packages/logger": "workspace:*",
    "@packages/tools": "workspace:*",
    "ai": "^5.0.56",
    "nestjs-zod": "^5.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3"
  }
}
